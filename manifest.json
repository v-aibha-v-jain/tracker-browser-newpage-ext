{
    "manifest_version": 3,
    "name": "Task Tracker",
    "version": "1.0.0",
    "description": "Daily task tracker with consistency analytics",
    "permissions": [
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}